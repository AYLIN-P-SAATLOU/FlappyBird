<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        x:Class="AvaloniaApplication1.MainWindow"
        Title="Flappy Bird" Width="400" Height="600"
        KeyDown="OnKeyDown">
    
    <Canvas Name="GameCanvas" Background="SkyBlue">
    <Ellipse Name="Bird" 
             Fill="Yellow" 
             Stroke="Black" 
             StrokeThickness="1.5"
             Width="34" Height="34" 
             Canvas.Left="50" Canvas.Top="200"/>
    </Canvas>
</Window>
